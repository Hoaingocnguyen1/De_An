MONGO_URI=
# Database name (default: multimodal_rag_db)
DB_NAME=multimodal_rag_db

VOYAGE_API_KEY=
# Voyage Models (optional, defaults are set in code)
VOYAGE_TEXT_MODEL=voyage-3
VOYAGE_MULTIMODAL_MODEL=voyage-multimodal-3
VOYAGE_RERANK_MODEL=rerank-2.5

GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash

# ==================== PROCESSING CONFIG ====================
# Maximum concurrent workers for parallel processing
# Processing
USE_VLM_EXTRACTION=true
VLM_FOR_TABLES=true
VLM_FOR_FIGURES=true
MAX_WORKERS=4
BATCH_SIZE=16

# Query
USE_QUERY_CACHE=true
INITIAL_K=20
FINAL_K=5
# Whisper model for video transcription
# Options: tiny, base, small, medium, large
WHISPER_MODEL=medium