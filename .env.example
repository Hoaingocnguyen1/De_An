# Example .env for Multimodal RAG (fill values before running)
# Copy this to .env and edit values.

# MongoDB connection string (replace <user>, <password>, <cluster> as needed)
MONGO_URI=mongodb+srv://<user>:<password>@cluster0.example.mongodb.net/multimodal_rag_db?retryWrites=true&w=majority

# Voyage AI (embeddings / reranker)
VOYAGE_API_KEY=pa-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
VOYAGE_MODEL=voyage-3
VOYAGE_MULTIMODAL_MODEL=voyage-multimodal-3

# Gemini (optional) â€” used for structured completions and synthesis
GEMINI_API_KEY=AIzaSyxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GEMINI_MODEL=gemini-2.1

# Optional/behavior tuning
DEFAULT_EMBEDDING_DIM=1024
BATCH_SIZE=32
MAX_WORKERS=4
LOG_LEVEL=INFO
